<fieldset class="fieldset_50">

<% form_tag producao_dentista_path do %>
    Entre : <%= date_select :data_inicio, @inicio, :order=>[:day, :month, :year] %>
    e : <%= date_select :data_fim, @fim , :order=>[:day,:month,:year] %><br/>
    Cl√≠nicas :  
    <% @dentista.clinicas.each() do |clinica| %>
      <%= check_box_tag "clinica_#{clinica.id}", clinica.id, checked=true %>  
    <%= clinica.nome %> &nbsp;&nbsp;
    <% end %>
  <p>
    <%= submit_tag 'pesquisar' %>
  </p>
<% end %>
</fieldset>
