<h1>Listing dentistas</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Telefone</th>
    <th>Celular</th>
    <th>Ativo</th>
  </tr>

<% @dentistas.each do |dentista| %>
  <tr>
    <td><%=h dentista.nome %></td>
    <td><%=h dentista.telefone %></td>
    <td><%=h dentista.celular %></td>
    <td><%=h dentista.ativo %></td>
    <td><%= link_to 'Show', dentista %></td>
    <td><%= link_to 'Edit', edit_dentista_path(dentista) %></td>
    <td><%= link_to 'Destroy', dentista, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New dentista', new_dentista_path %>