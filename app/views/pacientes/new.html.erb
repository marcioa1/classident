<% content_for :main do %>

<fieldset class="fieldset_50">
    <legend>Novo paciente</legend>
<% form_for(@paciente) do |f| %>
  <%= f.error_messages %>
  <%= render :partial=>'form' , :locals=>{:f=>f}%>
  <p>
    <%= f.submit 'Criar' %>
  </p>
<% end %>

<%= link_to 'voltar', pacientes_path %>
</fieldset>
<% end %>