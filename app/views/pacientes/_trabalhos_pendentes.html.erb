<h2>Trabalhos pendentes</h2>
<table>
  <tr>
    <th><br/>Data da envio</th>
    <th><br/>Descrição</th>
    <th><br/>Observação</th>
    <th><br/>Valor</th>
    <th>Data prevista<br/>de devolução</th>
    <th><br/>Data de devolução</th>
    <th><br/>Protético</th>
    <th><br/>Dentista</th>
  </tr>
<% @pendentes_protetico.each do |trab| %>
  <tr>
    <td><%= trab.data_de_envio.to_s_br%></td>
    <td><%= link_to trab.tabela_protetico.descricao, edit_trabalho_protetico_path(trab) unless trab.tabela_protetico.nil? %></td>
    <td><%= trab.observacoes %></td>
    <td><%= trab.valor.real %></td>
    <td><%= trab.data_prevista_de_devolucao.to_s_br%></td>
    <td><%= trab.data_de_devolucao.to_s_br %></td>
    <td><%= link_to trab.protetico.nome, abre_protetico_path(trab.protetico.id) %></td>
    <td><%= trab.dentista.nome unless trab.dentista.nil?%></td>

  </tr>

<% end %>
</table>